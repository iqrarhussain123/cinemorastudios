---
import Layout from "../layouts/Layout.astro";
import Navbar from "../components/Navbar.astro";
import Portfolio from "../components/Portfolio.astro";
import BookCall from "../components/BookCall.astro";
import Footer from "../components/Footer.astro";
---

<Layout
    title="Our Work | Cinemora Studios"
    description="See how we generate millions in pipeline revenue for elite coaches and founders."
>
    <Navbar />
    <main class="pt-20">
        <div class="px-4 py-20 text-center">
            <h1 class="font-heading text-5xl md:text-7xl text-white mb-6">
                Selected Work
            </h1>
            <p class="font-body text-zinc-400 text-xl max-w-2xl mx-auto">
                A collection of high-converting creative assets and strategies
                aimed at dominance.
            </p>
        </div>
        <Portfolio />
        <BookCall />
    </main>
    <Footer />
</Layout>
